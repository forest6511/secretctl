{
  "name": "frontend",
  "private": true,
  "version": "0.4.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "npx playwright test",
    "test:unit": "echo 'Vitest not yet configured - run: npm install -D vitest @testing-library/react'",
